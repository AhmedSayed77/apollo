{
  "scripts": {
    "start": "rm -rf .cache && gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "spell-check": "mdspell './source/**/*.md' './source/**/*.mdx' --en-us --ignore-acronyms --ignore-numbers --no-suggestions",
    "spell-check:ci": "npm run spell-check -- --report"
  },
  "dependencies": {
    "gatsby": "2.8.2",
    "gatsby-theme-apollo-docs": "1.0.2"
  },
  "devDependencies": {
    "markdown-spellcheck": "^1.3.1"
  }
}
